    "release": "bumpp && npm publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@cuiqg/eslint-config": "^2.1.9",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
